backend:
  name: git-gateway
  branch: main

media_folder: public/images
public_folder: /images

collections:
  - name: members
    label: Team Members
    folder: src/data/members.json
    format: json
    extension: ""
    create: false
    delete: false
    fields:
      - label: Team Members
        name: members
        widget: list
        fields:
          - {label: ID, name: id, widget: string}
          - {label: Name, name: name, widget: string}