# Netlify Identity Configuration
# Copy this file to .env and fill in your actual values

# Netlify Site URL (for identity configuration)
REACT_APP_NETLIFY_SITE_URL=https://your-site-name.netlify.app

# Admin email addresses (comma-separated)
# Users with these email addresses will be granted admin access
REACT_APP_ADMIN_EMAILS=admin@example.com,administrator@vitian.edu.in,cms.admin@racing.team

# Server-side admin emails for Netlify functions
ADMIN_EMAILS=admin@example.com,administrator@vitian.edu.in,cms.admin@racing.team

# Optional: Enable/disable invite-only mode
REACT_APP_INVITE_ONLY=true
