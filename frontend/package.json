{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000",
  "dependencies": {
    "@headlessui/react": "^2.2.3",
    "@heroicons/react": "^2.2.0",
    "@react-google-maps/api": "^2.20.6",
    "@reduxjs/toolkit": "^2.8.2",
    "@tailwindcss/postcss": "^4.1.7",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/netlify-identity-widget": "^1.9.6",
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@types/react-icons": "^2.2.7",
    "axios": "^1.9.0",
    "express": "^4.18.2",
    "netlify-identity-widget": "^1.9.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.6.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && node scripts/copy-data-files.js",
    "build-netlify": "CI=false NETLIFY=true react-scripts build && node scripts/copy-data-files.js || echo 'Build completed with warnings'",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "node server/server.js",
    "start:prod": "npm run build && npm run server",
    "rebuild-css": "npx tailwindcss -i ./src/index.css -o ./src/output.css && npm start",
    "copy-data": "node scripts/copy-data-files.js",
    "sync-data": "node scripts/sync-data.js",
    "dev": "npm run sync-data && concurrently \"npm run start\" \"npm run server\"",
    "cms": "npm run sync-data && npm run server"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17"
  }
}
